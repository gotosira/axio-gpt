{
  "name": "axio-gpt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:tokens": "node ./scripts/generate-css-from-export.mjs",
    "dev": "dotenv -e .env.local -- next dev",
    "build": "dotenv -e .env.local -- next build",
    "start": "dotenv -e .env.local -- next start",
    "lint": "eslint",
    "build:gh-pages": "GITHUB_PAGES=true next build && touch out/.nojekyll",
    "deploy:gh-pages": "npm run build:gh-pages && gh-pages -d out -b gh-pages -m 'deploy: GitHub Pages'",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.12",
    "@next-auth/prisma-adapter": "^1.0.7",
