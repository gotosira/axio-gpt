    "ai": "^5.0.44",
    "class-variance-authority": "^0.7.1",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.9",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "next-auth": "^4.24.11",
    "openai": "^5.20.2",
    "pdf-parse": "^1.1.1",
    "prisma": "^6.16.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "react-textarea-autosize": "^8.5.9",
    "rehype-highlight": "^7.0.2",
    "rehype-katex": "^7.0.0",
    "remark-gfm": "^4.0.1",
