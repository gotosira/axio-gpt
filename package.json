    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "4.1",
    "typescript": "^5"
  }
